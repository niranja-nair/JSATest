                               + (($("#adminsettings input.plotvisibleflag").is(':checked')) ? '&plotVisibleArea=true' : '')
                               + '&color=' + encodeURIComponent(MarkerColor);
                
                $('#adminsettings .baseobject-settings textarea.mapiturl').val(MapItURL);
                
