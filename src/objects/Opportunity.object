    <validationRules>
        <fullName>Inactive_Account_No_Orders</fullName>
        <active>false</active>
        <description>Inactive accounts can not have orders that go to QuickBooks</description>
        <errorConditionFormula>and(not( Account.Active__c  ),
or( ISPICKVAL(StageName, &apos;Closed Won&apos;),
ISPICKVAL(StageName,&apos;Posted&apos;),
ISPICKVAL(StageName,&apos;Grant Awarded&apos;),
ISPICKVAL(StageName,&apos;Ask - Accepted&apos;)))</errorConditionFormula>
        <errorMessage>An inactive account cannot have opportunities that are in a stage that sends them to QuickBooks.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Accept_if_Grad_Yr_Null</fullName>
        <active>false</active>
        <description>Do not allow Stage to be set to Accepted if Grad Yr. of Contact is not set (currently only applied to Summer School Registration record types).</description>
        <errorConditionFormula>AND(  $RecordType.Name=&quot;Summer School Registration&quot;,  Primary_Contact__r.High_School_Graduation_Year__c=&quot;&quot;)</errorConditionFormula>
        <errorMessage>Students cannot be accepted to programs if they have not yet set Graduation Year.  Check Graduation Year and JSA Role on the Primary Contact record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Campus_Session_Change_if_Accepted</fullName>
        <active>true</active>
        <description>Prevent changes to Campus/Session after Accepted.  Instead clone oppty.</description>
        <errorConditionFormula>AND(IsWon,
    ISCHANGED( Campus_Session__c ) 
   )</errorConditionFormula>
        <errorMessage>The Campus/Session field cannot be changed after the Opportunity is Accepted.  Please clone the Oppty, cancel the current one, then make sure all the information on the new oppty is correct.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_total_change_if_invoiced</fullName>
        <active>false</active>
        <description>Only opportunities that do not have an invoice in QuickBooks can be changed in a manner that changes their total</description>
        <errorConditionFormula>and(Invoice_Count__c &gt; 0,
PRIORVALUE(Amount ) &lt;&gt; Amount )</errorConditionFormula>
        <errorMessage>Cannot change an opportunity if it has been invoiced.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLD_No_change_if_invoiced</fullName>
        <active>false</active>
        <description>Only opportunities that do not have an invoice in QuickBooks can be changed.</description>
        <errorConditionFormula>OR(and(Invoice_Count__c &gt; 0,
PRIORVALUE(Invoice_Count__c) = Invoice_Count__c),
and(len(Invoice_Number__c ) &lt;&gt; 0,
PRIORVALUE(Invoice_Number__c) = Invoice_Number__c,
or(PRIORVALUE(Sales_Tax__c) = Sales_Tax__c,
isnull(PRIORVALUE(Sales_Tax__c)) = isnull(Sales_Tax__c)),
or(PRIORVALUE(Last_Payment_Date__c) = Last_Payment_Date__c,
isnull(PRIORVALUE(Last_Payment_Date__c)) = isnull(Last_Payment_Date__c)),
or(PRIORVALUE(Total_Payments__c) = Total_Payments__c,
isnull(PRIORVALUE(Total_Payments__c)) = isnull(Total_Payments__c)),
or(PRIORVALUE(Last_Credit_Date__c) = Last_Credit_Date__c,
isnull(PRIORVALUE(Last_Credit_Date__c)) = isnull(Last_Credit_Date__c)),
or(PRIORVALUE(Total_Credits__c) =  Total_Credits__c,
isnull(PRIORVALUE(Total_Credits__c)) = isnull(Total_Credits__c))
))</errorConditionFormula>
        <errorMessage>Cannot change an opportunity if it has been invoiced.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>no_invoice_change_after_QB</fullName>
        <active>false</active>
        <description>No change can be made to whether an opportunity is an invoice or a sales order after it has been sent to QuickBooks.</description>
        <errorConditionFormula>and(QuickBooks__c = true,
PRIORVALUE( Invoice__c ) &lt;&gt;  Invoice__c )</errorConditionFormula>
        <errorMessage>Cannot change what an opportunity will be in QuickBooks after it has been sent to QuickBooks.</errorMessage>
    </validationRules>
