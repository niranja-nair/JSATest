	margin: 0 auto;
}

.footerShadow
{
	 position: relative;
	float: left;
	width: 965px;
	height: 170px;
	-webkit-box-shadow: 0px 4px 5px rgba(50, 50, 50, 0.86);
	-moz-box-shadow:    0px 4px 5px rgba(50, 50, 50, 0.86);
	box-shadow:         0px 4px 5px rgba(50, 50, 50, 0.86);
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
	z-index: 100;
}

