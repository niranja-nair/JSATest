.mainBox
{
	position: relative;
	display: block;
	margin: 0 auto;
	width: 960px;
	margin-top: 10px;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
 }

.headerBox
{
 	 position: relative;
 	float: left;
 	width: 960px;
 	height: 177px;
 	background-color: #0C4384;
 	padding-left: 5px;
 	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
	-webkit-box-shadow: 0px 3px 6px rgba(50, 50, 50, 0.86);
	-moz-box-shadow:    0px 3px 6px rgba(50, 50, 50, 0.86);
	box-shadow:         0px 3px 6px rgba(50, 50, 50, 0.86);
	z-index: 500;
}

.topHeaderBox
{
	position: relative;
	width: 960px;
	float: left;
}

.topLogoBox
{
	margin-left: 5px;
	float: left;
}

.midBox
{
	display: block;
	background-color: #FFFFFF;
	margin-left: 2px;
	float: left;
	width:958px;
	min-height:331px;
	-webkit-box-shadow: 0px 3px 10px rgba(50, 50, 50, 0.86);
	-moz-box-shadow:    0px 3px 10px rgba(50, 50, 50, 0.86);
	box-shadow:         0px 3px 10px rgba(50, 50, 50, 0.86);
	z-index:-200;
}

.footerBox
{
	position: relative;
	float: left;
	width: 965px;
	height: 170px;
	background-color: #0C4384;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
	-webkit-box-shadow: 0px -1px 6px rgba(50, 50, 50, 0.86);
	-moz-box-shadow:    0px -1px 6px rgba(50, 50, 50, 0.86);
	box-shadow:         0px -1px 6px rgba(50, 50, 50, 0.86);
	z-index: 200;
}

.subfooterTextBox
{
	visibility: hidden;
	text-align: center;
