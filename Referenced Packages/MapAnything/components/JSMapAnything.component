//helper method for extracting nested objects using a dot notation field name
function extractValue(obj, field)
{
	try {
		var fieldParts = field.split('.');
		var currentObj = obj;
		for (var i = 0; i < fieldParts.length; i++)
		{
			currentObj = currentObj[fieldParts[i]];
		}
		return currentObj || '';
	}
	catch (err) { }
	
	return '';
}
    
