                var MapItURL = '{!BaseURL}?recordId=\{\!' + ObjectName + '.Id\}&baseObjectId=' + ObjectId + '&tooltipField=' + $("#tooltip1").val() 
                               + (($("#tooltip2").val() != '' && $("#tooltip2").val() != '--Please Choose--') ? ('&tooltipField2=' + $("#tooltip2").val()) : '')
